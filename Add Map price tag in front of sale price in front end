//add map price tag in front of the sale price in the website front-end
function bd_sale_price_html( $price, $product ) {

  if ( $product->is_on_sale() ) :

    $return_string = str_replace( '<ins>', '<ins> <div></div>Map: ', $price);
  return $return_string;

  else :
    return $price;
  endif;

}
add_filter( 'woocommerce_get_price_html', 'bd_sale_price_html', 100, 2 );
